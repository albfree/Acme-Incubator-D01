<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="conversion-service.xml" />

	<!-- HorrilloBulletin -->
	
	<bean id="HorrilloBulletin01" class="acme.entities.bulletins.HorrilloBulletin">
		<property name="author" value="Pedro Can"></property>
		<property name="body" value="Hello"></property>
		<property name="time" value="2019/09/09 10:00"></property>
	</bean>
	
	<bean id="HorrilloBulletin02" class="acme.entities.bulletins.HorrilloBulletin">
		<property name="author" value="Debra VanLunn"></property>
		<property name="body" value="Hello too"></property>
		<property name="time" value="2019/09/09 12:30"></property>
	</bean>
	
	<bean id="HorrilloBulletin03" class="acme.entities.bulletins.HorrilloBulletin">
		<property name="author" value="Margarett Diesel"></property>
		<property name="body" value="Bye"></property>
		<property name="time" value="2019/12/09 22:00"></property>
	</bean>


</beans>
